<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MyNewMobileApp.UserProfile" BackgroundColor="Maroon">
    <ContentPage.Content>
        <StackLayout>
       
          
            <StackLayout Orientation="Horizontal">
                <ImageButton  Source="gmail.png"  Clicked="btnGmail" />
                <ImageButton Source="facebook.png"  Clicked="btnFacebook" />
                <ImageButton Source="instagram.jpg"  Clicked="btnInstagram" />
                <ImageButton Source="linkedin.jpg"  Clicked="btnLinkedIn" />
            </StackLayout>
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>